              },
              "Effect": {
                "type": ["string"]
              },
              "Action": {
                "type": ["null", "array"],
                "items": {
                  "type": ["null", "string"]
                }
              },
              "Resource": {
                "type": ["null", "string"]
              }
            }
          }
        }
